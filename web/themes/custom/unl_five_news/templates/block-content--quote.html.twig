{% if content.field_quotemaster_link.0["#url"] %}
<div class="dcf-mb-6 custom-quote">
    <div class="custom-quote-box-left-border">{{ content.body }}</div>
    <div class="custom-credit custom-credit-link"><a href="{{ content.field_quotemaster_link.0["#url"] }}">{{ content.field_quotemaster }}</a></div>
    <span class="custom-credit-title">{{ content.field_quotemaster_title }}</span>
</div>

{% else %}
<div class="dcf-mb-6 custom-quote">
    <div class="custom-quote-box-left-border">{{ content.body }}</div>
    <div class="custom-credit ">{{ content.field_quotemaster }}</div>
    <span class="custom-credit-title">{{ content.field_quotemaster_title }}</span>
</div>

{% endif %}
