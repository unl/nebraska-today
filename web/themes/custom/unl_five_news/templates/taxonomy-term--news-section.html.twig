<div{{ attributes }}>
  {{ title_prefix }}
  {% if name and not page %}
    <h2><a href="{{ url }}">{{ name }}</a></h2>
  {% endif %}
  {{ title_suffix }}
</div>

<div>
  {{ drupal_view('taxonomy_term', 'block_2', term.id) }}
</div>

<div class="dcf-pt-8" >
  <div>
    {{ drupal_view('taxonomy_term', 'block_1', term.id) }}
  </div>
</div>

<div class="dcf-pt-8">
  <div class = "dcf-grid-halves@md dcf-grid-full@sm dcf-bleed dcf-ai-center unl-bg-lightest-gray custom-editors-pick-order">
    <div class="custom-editors-order-2">{{ drupal_view('editors_pick_2', 'block_1') }}</div>
    <div class="custom-editors-order-1">{{ drupal_view('editors_pick', 'block_1') }}</div>
  </div>
</div>




