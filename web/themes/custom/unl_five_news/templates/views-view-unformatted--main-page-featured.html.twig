{# Tweeter  #}
<div class="dcf-pt-8">
<hr class="custom-l-line dcf-mt-0 ">
<h2 class="dcf-txt-sm dcf-regular dcf-uppercase unl-ls-1">events</h2>
  <div class="custom-featured-events-grid">
    <div class="custom-events-image">{{ drupal_view('featured_event_image', 'block_1') }}</div>
    <div style="background-color: red; height: 100px; width:100%"></div>
  </div>
</div>

{# More headlines #}
<div class="dcf-pt-8 ">
  {{ drupal_view('recent', 'block_2') }}
</div>

